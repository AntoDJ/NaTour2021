<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NaTour2021" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="colorPrimary">@color/button</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/button</item>

        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.NaTour2021.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NaTour2021.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

    <style name="Theme.NaTour2021.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>